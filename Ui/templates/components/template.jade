mixin page(title, pagescript)
	- pagescript = pagescript || []
	doctype html
	html(lang="en")
		include head
		+head(title)
		body
			div.container
				h1= title
					img.brand-logo.pull-right(src="/Content/images/A.svg")

			block

			include scripts
			each script in pagescript
				script(src="/scripts/" + script + ".js")
